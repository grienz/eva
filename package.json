{
  "name": "eva",
  "version": "0.1.0",
  "private": true,
  "author": "Guray <grienz@protonmail.com>",
  "license": "MIT",
  "description": "Eva Smart Shower - Catalog Site",
  "repository": {
    "type": "git",
    "url": "https://github.com/grienz/eva.git"
  },
  "keywords": [
    "nextjs",
    "vercel",
    "tailwindcss",
    "sanity",
    "groq"
  ],
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "next build",
    "prebuild": "cd studio && yarn -s && yarn @sanity/cli build ../public/studio -y",
    "start": "next start",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "export": "next export",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next out coverage dist .swc",
    "lint": "next lint",
    "test": "jest --watch",
    "check-types": "tsc --noEmit --pretty",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/ibm-plex-sans": "^4.5.11",
    "@panelbear/panelbear-nextjs": "^1.0.3",
    "@portabletext/react": "^1.0.6",
    "@sanity/client": "^3.3.2",
    "@sanity/image-url": "^1.0.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "framer-motion": "^6.5.1",
    "groq": "^2.29.3",
    "next": "12.2.2",
    "@next/env": "^12.2.2",
    "next-intl": "^2.7.4",
    "next-seo": "^5.4.0",
    "next-themes": "^0.2.0",
    "nprogress": "^0.2.0",
    "pino": "^8.2.0",
    "pino-logflare": "^0.3.12",
    "preact": "^10.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@next/bundle-analyzer": "^12.2.2",
    "@tailwindcss/typography": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "18.0.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vercel/node": "^2.4.4",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "eslint": "8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
